#include<stdio.h>
int main ()
{ 
  int a,b,c;
  printf("enter the angles of the triangle (in degrees)\n:");
  printf("a = ");
  scanf("%d",&a);
  printf("b = ");
  scanf("%d",&b);
  printf("c = ");
  scanf("%d",&c);
  
  if (a+b+c!=180)
  printf("invalid analysis:\n");
  else if(a==b&&b==c)
  printf("equilateral triangle");
  else if (a==b||b==c||c==a)
  printf("isoselen triangle\n");
  else 
  printf("scalene triangle\n");
  return 0;
}
 
